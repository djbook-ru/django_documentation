# 9c457b382c86483681fe87c624f05d47
#: ../../ref/middleware.txt:189
msgid "CSRF protection middleware"
msgstr ""

# b47defcca6ec4db5b8c9ffde70715a31
#: ../../ref/middleware.txt:197
msgid ""
